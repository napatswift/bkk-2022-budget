filename="${1##*/}"
budim="bud-images/"
mkdir -p ${budim}
dir="$budim${filename%.*}"

mkdir -p ${dir}
rm -f ${dir}/*.jpg

convert -verbose \
        -density 190 \
        "$1" \
        -sharpen 0x1.0 \
        -quality 100 \
        -alpha remove -alpha off \
        -set filename:f '%t' \
        ${dir}/%03d.jpg

python3.9 to_text.py ${dir}